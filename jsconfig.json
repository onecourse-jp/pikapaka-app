    "paths": {
      "src/*": ["./src/*"],
      "@actions/*": ["./src/actions/*"],
      "@api/*": ["./src/api/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
