      "@lib/*": ["./src/lib/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@reducers/*": ["./src/reducers/*"],
      "@sagas/*": ["./src/sagas/*"],
      "@screens/*": ["./src/screens/*"],
      "@store/*": ["./src/store/*"],
